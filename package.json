{
  "name": "aws-sls-spa-sample-web",
  "version": "0.0.4",
  "scripts": {
    "docker-compose": "docker-compose -f dockers/docker-compose.yml",
    "docker-exec-ng": "npm run docker-compose -- exec angular",
    "docker-exec-t-ng": "npm run docker-compose -- exec -T angular",
    "docker-exec-shellcheck": "npm run docker-compose -- exec shellcheck",
    "docker-exec-t-shellcheck": "npm run docker-compose -- exec -T shellcheck",
    "docker-build": "npm run docker-compose -- build",
    "docker-up": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install --production",
    "docker-up-dev": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install",
    "docker-up-test": "npm run docker-compose -- up -d && npm run docker-exec-t-ng -- npm install",
    "docker-down": "npm run docker-compose -- down",
    "docker-test-all": "npm run docker-down && npm run docker-up-test && npm run docker-exec-t-shellcheck -- /shellcheck.sh && npm run docker-exec-t-ng -- npm run test-all && npm run docker-down",
    "docker-serve": "npm run docker-exec-ng -- ./bin/pre_build.sh && npm run docker-exec-ng -- ng serve --host=0.0.0.0",
    "ng": "ng",
    "start": "ng serve",
    "build": "./bin/pre_build.sh && ng build",
    "build-prod": "./bin/pre_build.sh && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "e2e-ci": "./bin/pre_build.sh && ng e2e -c ci",
    "test-all": "npm-run-all lint build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.5",
    "@angular/cdk": "^8.1.4",
    "@angular/common": "^8.2.5",
    "@angular/compiler": "^8.2.5",
    "@angular/core": "^8.2.5",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.5",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser": "^8.2.5",
    "@angular/platform-browser-dynamic": "^8.2.5",
    "@angular/router": "^8.2.5",
    "@ngrx/effects": "^8.3.0",
    "@ngrx/store": "^8.3.0",
    "angular-in-memory-web-api": "^0.8.0",
    "aws-amplify": "^1.1.38",
    "core-js": "^3.2.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.3",
    "uuid": "^3.3.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.801.3",
    "@angular/cli": "^8.3.3",
    "@angular/compiler-cli": "^8.2.5",
    "@angular/language-service": "^8.2.5",
    "@aws-amplify/ui": "^1.0.23",
    "@types/graphql": "^14.5.0",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.13.20",
    "@types/uuid": "^3.4.5",
    "angular-tslint-rules": "^1.18.0",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
