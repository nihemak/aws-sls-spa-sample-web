{
  "name": "aws-sls-spa-sample-web",
  "version": "0.0.2",
  "scripts": {
    "docker-compose": "docker-compose -f dockers/docker-compose.yml",
    "docker-exec-ng": "npm run docker-compose -- exec angular",
    "docker-exec-shellcheck": "npm run docker-compose -- exec shellcheck",
    "docker-build": "npm run docker-compose -- build",
    "docker-up": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install --production",
    "docker-up-dev": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install",
    "docker-down": "npm run docker-compose -- down",
    "docker-test-all": "npm run docker-down && npm run docker-up-dev && npm run docker-exec-shellcheck -- /shellcheck.sh && npm run docker-exec-ng -- npm run test-all && npm run docker-down",
    "docker-serve": "npm run docker-exec-ng -- ./bin/pre_build.sh && npm run docker-exec-ng -- ng serve --host=0.0.0.0",
    "ng": "ng",
    "start": "ng serve",
    "build": "./bin/pre_build.sh && ng build",
    "build-prod": "./bin/pre_build.sh && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "test-all": "npm-run-all lint build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "angular-in-memory-web-api": "^0.8.0",
    "aws-amplify": "^1.1.28",
    "core-js": "^3.1.3",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.2",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@aws-amplify/ui": "^1.0.19",
    "@types/graphql": "^14.2.0",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.13.13",
    "@types/uuid": "^3.4.4",
    "angular-tslint-rules": "^1.14.0",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "ts-node": "^8.1.1",
    "tslint": "^5.17.0",
    "typescript": "^3.2.4"
  }
}
