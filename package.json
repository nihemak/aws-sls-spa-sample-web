{
  "name": "aws-sls-spa-sample-web",
  "version": "0.0.0",
  "scripts": {
    "docker-compose": "docker-compose -f dockers/docker-compose.yml",
    "docker-exec-ng": "npm run docker-compose -- exec angular",
    "docker-build": "npm run docker-compose -- build",
    "docker-up": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install --production",
    "docker-up-dev": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install",
    "docker-down": "npm run docker-compose -- down",
    "docker-test-all": "npm run docker-down && npm run docker-up-dev && npm run docker-exec-ng -- npm run test-all && npm run docker-down",
    "docker-serve": "npm run docker-exec-ng -- ng serve --host=0.0.0.0",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "test-all": "npm-run-all lint build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/router": "^6.1.9",
    "core-js": "^2.5.4",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.3",
    "@angular/cli": "^6.2.3",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "^2.0.4",
    "@types/node": "^10.10.3",
    "angular-tslint-rules": "^1.5.0",
    "codelyzer": "~4.4.4",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
