{
  "name": "aws-sls-spa-sample-web",
  "version": "0.0.2",
  "scripts": {
    "docker-compose": "docker-compose -f dockers/docker-compose.yml",
    "docker-exec-ng": "npm run docker-compose -- exec angular",
    "docker-exec-shellcheck": "npm run docker-compose -- exec shellcheck",
    "docker-build": "npm run docker-compose -- build",
    "docker-up": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install --production",
    "docker-up-dev": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install",
    "docker-down": "npm run docker-compose -- down",
    "docker-test-all": "npm run docker-down && npm run docker-up-dev && npm run docker-exec-shellcheck -- /shellcheck.sh && npm run docker-exec-ng -- npm run test-all && npm run docker-down",
    "docker-serve": "npm run docker-exec-ng -- ./bin/pre_build.sh && npm run docker-exec-ng -- ng serve --host=0.0.0.0",
    "ng": "ng",
    "start": "ng serve",
    "build": "./bin/pre_build.sh && ng build",
    "build-prod": "./bin/pre_build.sh && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "test-all": "npm-run-all lint build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "angular-in-memory-web-api": "^0.6.1",
    "aws-amplify": "^1.0.10",
    "core-js": "^2.6.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.3.3",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.8",
    "@angular/cli": "^6.2.8",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@aws-amplify/ui": "^1.0.10",
    "@types/graphql": "^14.0.3",
    "@types/jasmine": "^2.8.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.12",
    "@types/uuid": "^3.4.4",
    "angular-tslint-rules": "^1.5.0",
    "codelyzer": "~4.4.4",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
