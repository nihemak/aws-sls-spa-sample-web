{
  "name": "aws-sls-spa-sample-web",
  "version": "0.0.2",
  "scripts": {
    "docker-compose": "docker-compose -f dockers/docker-compose.yml",
    "docker-exec-ng": "npm run docker-compose -- exec angular",
    "docker-exec-shellcheck": "npm run docker-compose -- exec shellcheck",
    "docker-build": "npm run docker-compose -- build",
    "docker-up": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install --production",
    "docker-up-dev": "npm run docker-compose -- up -d && npm run docker-exec-ng -- npm install",
    "docker-down": "npm run docker-compose -- down",
    "docker-test-all": "npm run docker-down && npm run docker-up-dev && npm run docker-exec-shellcheck -- /shellcheck.sh && npm run docker-exec-ng -- npm run test-all && npm run docker-down",
    "docker-serve": "npm run docker-exec-ng -- ./bin/pre_build.sh && npm run docker-exec-ng -- ng serve --host=0.0.0.0",
    "ng": "ng",
    "start": "ng serve",
    "build": "./bin/pre_build.sh && ng build",
    "build-prod": "./bin/pre_build.sh && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "test-all": "npm-run-all lint build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.1.2",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/router": "^8.1.2",
    "@ngrx/effects": "^8.1.0",
    "@ngrx/store": "^8.1.0",
    "angular-in-memory-web-api": "^0.8.0",
    "aws-amplify": "^1.1.32",
    "core-js": "^3.1.4",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.2",
    "uuid": "^3.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "^8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/language-service": "^8.1.2",
    "@aws-amplify/ui": "^1.0.22",
    "@types/graphql": "^14.2.3",
    "@types/jasmine": "^3.3.15",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.13.18",
    "@types/uuid": "^3.4.5",
    "angular-tslint-rules": "^1.18.0",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.4.5"
  }
}
